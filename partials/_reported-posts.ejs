<table class="table">
  <tr>
    <th>Number of Reports</th>
    <th>Description</th>
    <th>Reason for Report</th>
    <th>Post Identifier</th>
    <th>Owner</th>
    <th>Owner Identifier</th>
  </tr>
  <% reportedPosts.forEach(function(post){ %>
    <tr id="reportedPostRow_<%= post.uid %>">
      <td><%= numberOfPostReports[post.uid] %></td>
      <td> <%= post.description %> </td>
      <td><%= postReasonList[post.uid] %> </td>
      <td><%= post.uid %> </td>
      <td> <%= `${post.owner_first_name} ${post.owner_last_name}` %> </td>
      <td> <%= post.owner_uid %> </td>
      <td><button onClick="ignoreReportedPost('<%= post.uid %>')" type="button" class="btn btn-outline-warning">Ignore</button></td>
      <td><button onClick="removeReportedPost('<%= post.uid %>')" type="button" class="btn btn-outline-danger">Delete Post</button></td>
    </tr> 
  <% }); %>
</table>

    